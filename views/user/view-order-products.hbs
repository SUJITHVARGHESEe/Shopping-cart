{{!-- <h1>{{title}}</h1>
<p>Welcome to {{title}}</p> --}}
<div class="container mt-5">
    <div class="row">
       {{#each products}}
        <div class="col-md-3 pt-3">
<div class="card" style="width: 15rem; height:22rem">
  <img class="card-img-top " style="height: 10rem;" src="/product-images/{{this.product._id}}.jpg" alt="Card image cap">
  <div class="card-body">
    <h5 class="card-title">{{this.product.name}}</h5>
    <p class="card-title">Rs.{{this.product.price}}</p>
    <p class="card-text">quantity:{{this.quantity}}</p>
     <p class="card-text">{{this.product.Description}}</p>
  </div>
</div>
</div>
{{/each}}
</div>
</div>

